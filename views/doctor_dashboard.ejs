    <div class="container">
        <div class="row">
            <div class="col">
             <div class="profile">
                <div class="col">
                    <div class="col">
                        <h2><%= d.user_id.name %> </h2>
                        <p><strong>Appointment Date: </strong><%= d.appointment_date %>   </p>
                        <p><strong>Time Slot: </strong><%= d.time_slot %> </p>
                        
                            
                        <button class="btn">Confirm Appointment</button>
                        <button class="btn">Cancel Appointment</button>  
                    </div>             
                    
                    </div>
                </div>
                </div>
                </div>
                </div>


<% })%>
<%- include('./partials/footer');-%>